<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="akka"
             type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>

  <akka>
    <hocon>
      <![CDATA[
          akka {
              stdout-loglevel = DEBUG
              loglevel = DEBUG
              actor.debug.unhandled = on
          }
      ]]>
    </hocon>
  </akka>
</configuration>


